{
  "extends": "standard",
  "plugins": [
    "react",
    "prettier"
  ],
  "rules": {
    "no-return-assign": 0,
    "space-before-function-paren": 0,
    "semi": 0,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "prettier/prettier": ["error", {"singleQuote": true}]
  },
  "globals": {
  }
}
